viewer, Stars: { ...value.Review.Stars, Friendly: newValue}}})}}\n                                icon={<FavoriteIcon fontSize=\"inherit\" />}\n                            /></h5> \n\n                            <Divider orientation=\"center\"><h5>คะแนนรวม: {finalStars} <FavoriteIcon style={{color: \"#ff6d75\"}}/></h5> </Divider>\n\n                    <h5 className=\"mt-5\">ความคิดเห็นเพิ่มเติม</h5>\n                    <TextArea rows={4} value={value.Review.Comment} showCount maxLength={100} onChange={(e)=>setValue({Review:{...value.Review, Comment: e.target.value}})} />\n\n                    <h5 className=\"mt-5\">ผู้รีวิว: {info? (info.Review.Reviewer):(null)}</h5>\n                </div>\n            </Modal>\n        </div>\n    )\n}\n\nexport default Booking\n","/Users/iris/majoramassage/src/Components/Parts/Header.js",["204","205","206","207","208","209","210"],"/Users/iris/majoramassage/src/App.js",[],"/Users/iris/majoramassage/src/Components/Parts/Footer.js",["211","212","213","214"],"/Users/iris/majoramassage/src/reportWebVitals.js",[],"/Users/iris/majoramassage/src/Components/cropImage.js",[],"/Users/iris/majoramassage/src/Components/AdminPage.js",["215","216","217","218","219"],{"ruleId":"220","replacedBy":"221"},{"ruleId":"222","replacedBy":"223"},{"ruleId":"224","severity":1,"message":"225","line":3,"column":8,"nodeType":"226","messageId":"227","endLine":3,"endColumn":11},{"ruleId":"224","severity":1,"message":"228","line":3,"column":26,"nodeType":"226","messageId":"227","endLine":3,"endColumn":33},{"ruleId":"224","severity":1,"message":"229","line":16,"column":20,"nodeType":"226","messageId":"227","endLine":16,"endColumn":31},{"ruleId":"224","severity":1,"message":"230","line":31,"column":13,"nodeType":"226","messageId":"227","endLine":31,"endColumn":21},{"ruleId":"231","severity":1,"message":"232","line":69,"column":1,"nodeType":"233","messageId":"234","endLine":69,"endColumn":48},{"ruleId":"231","severity":1,"message":"232","line":96,"column":1,"n